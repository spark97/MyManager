<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mnnit.harshitshah.mymanager" >
    <uses-permission android:name="com.android.alarm.permission.SET_ALARM"/>
    <receiver
        android:name=".AlarmReceiver"
        android:enabled="true"
        android:exported="true" >
        <intent-filter>
            <action android:name="mnnit.harshitshah.mymanager"/>
        </intent-filter>
    </receiver>
    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name=".check"
            android:label="@string/title_activity_check" >
        </activity>
        <activity
            android:name=".loggedin"
            android:label="@string/title_activity_loggedin" >
        </activity>
        <activity
            android:name=".newbudget"
            android:label="@string/title_activity_newbudget" >
        </activity>
        <activity
            android:name=".manage"
            android:label="@string/title_activity_manage" >
        </activity>
        <activity
            android:name=".savings"
            android:label="@string/title_activity_savings" >
        </activity>
        <activity
            android:name=".DrawerActivity"
            android:label="@string/title_activity_drawer" >
        </activity>
        <activity
            android:name=".DrawerAct1"
            android:label="@string/title_activity_drawer_act1" >
        </activity>
        <activity
            android:name=".DrawerAct2"
            android:label="@string/title_activity_drawer_act2" >
        </activity>
        <activity
            android:name=".storenote"
            android:label="storenote" >
        </activity>
    </application>

</manifest>
